<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUIPages.Pages.NavPageDemo.NavContentPage1"
             Title="NavContentPage1"
             BackgroundColor="#611726">
    <VerticalStackLayout>
        <Label 
            Text="This is Nav content page 1"
            FontSize="20"
            TextColor="White"
            FontAttributes="Bold"
            Margin="20"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button 
            x:Name="navContentPage2Button"
            Text="Open Content Page 2"
            Clicked="navContentPage2Button_Clicked"
            Margin="20"
        >
        </Button>
    </VerticalStackLayout>
</ContentPage>